<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  <display-name>FC_Semi</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>EmailSendServlet</servlet-name>
    <servlet-class>com.nbp.common.EmailSendServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmailSendServlet</servlet-name>
    <url-pattern>/common/emailSend.do</url-pattern>
  </servlet-mapping>
  <servlet>
        <servlet-name>jsonServlet</servlet-name>
        <servlet-class>com.example.JsonServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>jsonServlet</servlet-name>
        <url-pattern>/json</url-pattern>
    </servlet-mapping>
  
</web-app>

